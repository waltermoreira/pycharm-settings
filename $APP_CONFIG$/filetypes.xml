<application>
  <component name="FileTypeManager" version="11">
    <ignoreFiles list="CVS;SCCS;RCS;rcs;.DS_Store;.svn;.pyc;.pyo;*.pyc;*.pyo;.git;*.hprof;_svn;.hg;*.lib;*~;__pycache__;.bundle;*.rbc;*$py.class;" />
    <extensionMap>
      <mapping ext="conf" type="ARCHIVE" />
      <mapping ext="yml" type="PLAIN_TEXT" />
    </extensionMap>
    <setting name="fileTypeChangedCounter" value="2" />
  </component>
</application>